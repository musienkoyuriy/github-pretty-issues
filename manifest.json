{
  "name": "github-pretty-issues",
  "short_name": "github-pretty-issues",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Find the right answers faster",
  "homepage_url": "http://github.com/musienkoyuriy/github-pretty-issues",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["https://github.com/*/issues/*"],
      "js": ["pretty-issues.js"]
    }
  ]
}
